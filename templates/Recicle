{#<h3>ESTUDIOS </h3>#}
{#<table class="tabla">#}
{#	<thead>#}
{#        <tr>#}
{#			<th class="encabezado">ID</th>#}
{#			<th class="encabezado">Titulo</th>#}
{#            <th class="encabezado">Fecha Inici</th>#}
{#            <th class="encabezado">Fecha Fin</th>#}
{#            <th class="encabezado">Ciudad</th>#}
{#			#}
{#        </tr>#}
{#		</thead>#}
{#		<tbody>#}
{#        {% for r in estudios %}#}
{#        <tr>#}
{#            <td>{{ r.id }}</td>#}
{#            <td>{{ r.fk_Detalle_Estudio.titulo }}</td>  <!-- fk_Detalle_Estudio es el ForeignKey a Estudio -->#}
{#            <td>{{ r.fk_Detalle_Estudio.fechaInicio |date:"d/m/Y" }}</td> <!-- Asegúrate de usar los nombres correctos de los campos -->#}
{#            <td>{{ r.fk_Detalle_Estudio.fechaFin  |date:"d/m/Y"}}</td>#}
{#            <td>{{ r.fk_Detalle_Estudio.ciudad }}</td>#}
{#        </tr>#}
{#        {% endfor %}#}
{##}
{#		 </tbody>#}
